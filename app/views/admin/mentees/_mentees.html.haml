%table.table-striped
  %tr
    %th
      First Name
    %th
      Last Name
    %th
      Department
    %th
      Location
    %th
      Available
    %th
      Matches

  - mentees.each do |m|
    %tr
      %td
        = m.profile.first_name
      %td
        = m.profile.last_name
      %td
        = m.profile.department
      %td
        = m.profile.location
      %td
        = m.mentee_profile.is_available ? 'Yes' : 'No'
      %td
        = link_to 'Matches', "/admin/users/#{m.id}/matches"
